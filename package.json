{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Asary",
  "license": "MIT",
  "scripts": {
    "tsc": "tsc",
    "build": "cross-env NODE_ENV=production tsc && ef-tspm",
    "serve:dev": "cross-env NODE_ENV=development && ts-node-dev -r tsconfig-paths/register --respawn --transpileOnly ./src/index.ts",
    "serve:local": "cross-env NODE_ENV=production node ./build/index.js",
    "serve:pm2": "NODE_ENV=production pm2 start ./build",
    "seed:run": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.6",
    "@types/faker": "^4.1.12",
    "@types/node": "^8.0.29",
    "ts-node": "3.3.0",
    "tsc": "^1.20150623.0",
    "tslint": "^6.1.2",
    "typescript": "3.3.3333"
  },
  "dependencies": {
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@ef-carbon/tspm": "^2.2.5",
    "@sindresorhus/slugify": "^1.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.6",
    "@types/express-serve-static-core": "^4.17.8",
    "@types/helmet": "^0.0.47",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/multer": "^1.4.3",
    "@types/underscore": "^1.10.3",
    "@types/uuid": "^7.0.2",
    "awesome-typescript-loader": "^5.2.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.1",
    "chalk": "^4.1.0",
    "class-validator": "^0.12.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.2",
    "del": "^5.1.0",
    "dotenv": "^8.2.0",
    "express": "^4.15.4",
    "express-validator": "^6.6.0",
    "faker": "^4.1.0",
    "helmet": "^3.23.3",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "multer": "^1.4.2",
    "mysql": "^2.14.1",
    "reflect-metadata": "^0.1.10",
    "slugify": "^1.4.4",
    "ts-node-dev": "^1.0.0-pre.50",
    "tsconfig-paths": "^3.9.0",
    "tspath": "^1.3.7",
    "typeorm": "0.2.24",
    "typeorm-seeding": "^1.6.1",
    "underscore": "^1.10.2",
    "uuid": "^8.2.0"
  },
  "_moduleAliases": {
    "@database/*": "build/database/*",
    "@controllers/*": "build/controllers/*",
    "@middlewares/*": "build/app/middlewares/*",
    "@routes/*": "build/app/routes/*",
    "@util/*": "build/app/utils/*",
    "@config/*": "build/config/*",
    "@auth/*": "build/app/modules/auth/*",
    "@admin/*": "build/app/modules/admin/*",
    "@freelancer/*": "build/app/modules/freelancer/*",
    "@helper/*": "build/app/modules/helper/*"
  }
}
